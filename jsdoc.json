{
	"tags": {
		"allowUnknownTags": false
	},
	"plugins": [
	],
	"opts": {
		"template": "node_modules/docdash",
		"encoding": "utf8",
		"destination": "./docs",
		"recurse": true
    },
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		},
		"navType": "vertical",
		"linenums": true,
		"dateFormat": "MMMM Do YYYY, h:mm:ss a"
	},
	"docdash": {
        "static": true,
        "sort": true,
        "sectionOrder": [
             "Classes",
             "Modules",
             "Externals",
             "Events",
             "Namespaces",
             "Mixins",
             "Tutorials",
             "Interfaces"
        ],
        "meta": {
            "title": "Civitas game",
            "description": "Civitas is an empire-building game written in Javascript with the help of the jQuery library."
        },
        "search": true,
        "collapse": true,
        "wrap": false,
        "typedefs": true,
        "navLevel": 10,
        "scripts": [],
        "menu": {
            "sizeof(cat)": {
                "href": "https://sizeof.cat",
                "target": "_blank",
                "class": "menu-item",
                "id": "site_link"
            },
            "GitHub": {
                "href": "https://github.com/sizeofcat/civitas",
                "target": "_blank",
                "class": "menu-item",
                "id": "github_link"
            }
        }
    }
}
